{
  "name": "primate",
  "version": "0.32.0~next",
  "description": "Polymorphic development platform",
  "homepage": "https://primatejs.com",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "src/defaults/*.html",
    "!src/**/*.spec.js",
    "types/*.ts"
  ],
  "bin": "src/bin.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/primate"
  },
  "dependencies": {
    "@primate/core": "file:../core",
    "@rcompat/async": "^0.1.1",
    "@rcompat/fs": "^0.1.1",
    "rcompat": "^0.17.0"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "types": "./types/index.d.ts",
  "exports": {
    ".": "./src/exports.js",
    "./errors": "./src/errors.js"
  },
  "scripts": {
    "prepublishOnly": "dts-buddy"
  }
}
