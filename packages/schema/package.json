{
  "name": "@primate/schema",
  "version": "0.1.1",
  "description": "Primate schema validation",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/schema"
  },
  "dependencies": {
    "@rcompat/invariant": "^0.7.0",
    "@rcompat/object": "^0.5.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": "./src/private/*.js",
    "#predicates/*": "./src/private/predicates/*.js"
  },
  "exports": {
    "./*": "./src/public/*.js"
  }
}
